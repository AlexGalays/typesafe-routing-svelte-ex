{
  "name": "svelte-app",
  "version": "1.0.0",

  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public -s -q -p 8888",
    "validate": "svelte-check"
  },

  "devDependencies": {
    "@rollup/plugin-commonjs": "17.0.0",
    "@rollup/plugin-node-resolve": "11.0.1",
    "rollup": "2.35.1",
    "rollup-plugin-livereload": "2.0.0",
    "rollup-plugin-svelte": "7.0.0",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-css-only": "3.1.0",
    "svelte": "3.31.0",
    "svelte-check": "1.1.23",
    "svelte-preprocess": "4.6.1",
    "@rollup/plugin-typescript": "8.1.0",
    "typescript": "4.1.3",
    "tslib": "2.0.3",
    "sirv-cli": "1.0.7",
    "prettier": "2.1.2"
  },

  "dependencies": {
    "typescript-router": "0.0.3",
    "idonttrustlikethat": "1.0.5"
  },

  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "arrowParens": "avoid",
    "useTabs": false
  }
}
